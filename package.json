{
  "name": "demo-ecommerce-backend2",
  "description": "Demo eCommerce BackEnd - API supplier and Swagger Docs",
  "version": "2.0.0",
  "private": false,
  "license": "MIT",
  "readme": "README.md",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "pm2 start ./pm2-local.config.js && pm2 logs",
    "start-production": "pm2 start ./pm2-production.config.js && pm2 logs",
    "test": "mocha -r ts-node/register",
    "test-report": "nyc npm run test -- --reporter mochawesome --reporter-options reportDir='./build/test-results/',reportFilename='index',reportPageTitle='demo-ecommerce-test mocha test results',inline=true,cdn=true,charts=true,autoOpen=false",
    "build": "rimraf ./build && tsc --build tsconfig.json && npm run test-report",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx}"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "ajv": "6.12.6",
    "dotenv": "8.2.0",
    "fastify": "3.14.2",
    "fastify-cors": "5.2.0",
    "fastify-env": "2.1.0",
    "fastify-helmet": "5.3.1",
    "fastify-plugin": "3.0.0",
    "fastify-swagger": "4.6.0",
    "json-schema-to-ts": "1.6.1",
    "json5": "2.2.0",
    "knex": "0.21.19",
    "objection": "2.2.15",
    "objection-softdelete": "3.0.1",
    "pg": "8.6.0",
    "pm2": "4.5.6",
    "rimraf": "3.0.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@types/chai": "4.2.16",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.41",
    "@types/pg": "7.14.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "chai": "4.3.4",
    "chai-string": "1.5.0",
    "eslint": "7.24.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-jsdoc": "32.3.0",
    "husky": "6.0.0",
    "mocha": "8.3.2",
    "mochawesome": "6.2.2",
    "nyc": "15.1.0",
    "sinon": "10.0.1",
    "sinon-chai": "3.6.0",
    "ts-node": "9.1.1"
  }
}
